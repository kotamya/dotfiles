{
    "editor.lineHeight": 0,
    "editor.fontSize": 12,
    "editor.fontFamily": "Meslo LG M",
    "editor.wordWrap": "on",
    "files.insertFinalNewline": true,
    "python.pythonPath": "$HOME/Anaconda3/python.exe",
    "terminal.integrated.fontSize": 12,
    "workbench.colorTheme": "Iceberg",
    "terminal.integrated.fontFamily": "Meslo LG M",
    "terminal.external.windowsExec": "$HOME/../../Program Files/Git/git-bash.exe",
    "window.zoomLevel": 0,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "jupyter.askForKernelRestart": false,
    "editor.inlineSuggest.enabled": true
}
